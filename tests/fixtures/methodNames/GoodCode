<?php

namespace HexletPsrLinter;

/**
 * Code with right method names
 */
class BadCode
{
    public function __construct($argument)
    {
        echo "__construct";
    }

    public function fakeMethod1()
    {
        echo "fakeMethod1";
    }

    public function fakeMethod2($x)
    {
        echo "fakeMethod2 ".strval($x);
    }

    public function __destruct()
    {
        echo "__destruct";
    }
}
